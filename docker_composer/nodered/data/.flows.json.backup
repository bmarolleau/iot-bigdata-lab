        "id": "6b6ddf244bc3c0b6",
        "type": "function",
        "z": "f6f2187d.f17ca8",
        "name": "function 1",
        "func": "// Initialize x in the context if it doesn't exist\nif (context.get('x') === undefined) {\n    context.set('x', 1);\n}\n\n// Get the current value of x\nlet x = context.get('x');\n\n// Create the payload\nmsg.payload = '{\"temp\":' + x + ', \"humidity\":1, \"objectTemp\":5}';\n\n// Increment x and store it back in the context\nx = x*1.03;\ncontext.set('x', x);\n\n// Return the message\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
